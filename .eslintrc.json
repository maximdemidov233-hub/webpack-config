{
    "env": {
        // "browser": true,
        // "commonjs": true,
        // "es2021": true,
        "node": true,
        "commonjs": true,
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["warn", "single"],
        "indent": ["warn", 4],
        // "semi": "on"
        "semi": ["warn", "always"]
    }
}
